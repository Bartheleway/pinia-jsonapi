{
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "USAGE.md",
    "recurse": true,
    "template": "node_modules/better-docs/",
    "tutorials": "tutorials/"
  },
  "markdown": {
    "idInHeadings": true
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./src"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules|docs)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "better-docs": {
      "name": "jsonapi-vuex documentation",
      "logo": "docs/logo.png",
      "navigation": [
        {
          "label": "Github",
          "href": "https://github.com/mrichar1/jsonapi-vuex"
        }
      ]
    }
  }
}
