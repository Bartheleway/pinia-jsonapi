{
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "tutorials": "tutorials/"
  },
  "markdown": {
    "idInHeadings": true
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./src"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules|docs)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true
  }
}
