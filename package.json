    "chromedriver": "^103",
    "concurrently": "^6.2.1",
    "core-js": "^3.16.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^7.16.0",
    "fake-json-api-server": "^1.6.0",
    "geckodriver": "^3.0.1",
    "husky": "^7.0.1",
    "jsdoc": "^3.6.6",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.1",
    "karma-mocha": "^2.0.1",
