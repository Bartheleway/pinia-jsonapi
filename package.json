    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "mocha-eslint": "^7.0.0",
    "nightwatch": "^2.3.0",
    "prettier": "^2.3.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "vue": "^3.2.37",
    "vuex": "^4.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.11",
    "vuex": "^4.0.0"
  },
  "bugs": {
